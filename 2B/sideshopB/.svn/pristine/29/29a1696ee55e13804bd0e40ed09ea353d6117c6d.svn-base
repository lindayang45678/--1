html,body,div,span,applet,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,abbr,acronym,address,big,cite,code,del,dfn,em,img,ins,kbd,q,s,samp,small,strike,strong,sub,sup,tt,var,b,u,i,center,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,input,button,textarea,table,caption,tbody,tfoot,thead,tr,th,td,article,aside,canvas,details,figcaption,figure,footer,header,hgroup,menu,nav,section,summary,time,mark,audio,video{margin:0;padding:0;border:0;}
html,body,div,h1,h2,h3,h4,h5,h6,p,dl,dt,dd,ol,ul,li,fieldset,form,input,button,textarea,table,caption,tbody,tfoot,thead,tr,th,td{outline:none;}
html body{height:100%;width:100%;min-width:320px;margin:0 auto;font:12px/1.5 'Microsoft YaHei',"宋体","arial","tahoma","simsun";color:#323232;background:#f2f2f2;}
article,aside,details,figcaption,figure,footer,header,hgroup,nav,section{display:block;outline:none;}
audio,canvas,video{display:inline-block;*display:inline;*zoom:1;}
audio:not([controls]){display:none;}
ul,ol,li{list-style:none;text-align:left;}
img{height:auto;vertical-align:top;border:0 none;outline:none;}
input,label,button,textarea,select{border:medium none;font-family:"microsoft yahei";}
input[type="submit"],input[type="text"],input[type="button"],input[type="password"],input[type="search"],input[type="number"],input[type="tel"]{-webkit-appearance:none;appearance:none;-webkit-tap-highlight-color:rgba(0, 0, 0, 0);-webkit-tap-highlight-color:transparent;line-height:normal;}
input[type="number"]::-webkit-inner-spin-button,input[type="number"]::-webkit-outer-spin-button{-webkit-appearance:none;appearance:none;margin:0;}
input::-webkit-input-placeholder{color:#999;}
input::-moz-placeholder{color:#999;}
input::-moz-placeholder{color:#999;}
input::-ms-input-placeholder{color:#999}
input:focus{outline:0;}
table{border-collapse:collapse;border-spacing:0;}
em,i,s,del,small,u,strike{font-style:normal;font-weight:normal;text-decoration:none;}
a{outline:0;text-decoration:none;color:#f04444;-webkit-tap-highlight-color:rgba(0,0,0,0);tap-highlight-color:rgba(0,0,0,0);}
a:active,a:focus{outline:0;}
.body-white{background:#fff;}
.color{color:#f04444;}
.c-white{color:#fff;}
.c-red{color:#f00;}
.c-black{color:#0c0c0c;}
.c-gray{color:#838383;}
.c-orange{color:#f99406;}
.c-blue{color:#3598db;}
.c-color{color:#f05555;}
.f-10{font-size:10px;}
.f-13{font-size:13px;}
.f-14{font-size:14px;}
.f-16{font-size:16px;}
.f-18{font-size:18px;}
.clear{clear:both;}
.clearfix:before,.clearfix:after{content:' ';display:block;overflow:hidden;visibility:hidden;width:0;height:0;}
.clearfix:after{clear:both;}
.clearfix{zoom:1;}
.fl{float:left;}
.fr{float:right;}
.btn-red{display:block;width:100%;height:48px;line-height:48px;letter-spacing:10px;text-align:center;font-size:16px;-webkit-border-radius:3px;border-radius:3px;color:#fff;background:#f04444;}
.btn-white{display:block;height:44px;line-height:44px;letter-spacing:10px;text-align:center;font-size:16px;-webkit-border-radius:3px;border-radius:3px;color:#f04444;background:#fff;border:2px solid #f04444;}
.btn-gray{display:block;width:100%;height:48px;line-height:48px;letter-spacing:10px;text-align:center;font-size:16px;-webkit-border-radius:3px;border-radius:3px;color:#fff;background:#929292;}
.btn-red:visited,.btn-gray:visited{color:#fff;}
.btn-white:visited{color:#f04444;}
.focus{filter:alpha(opacity=80);-moz-opacity:0.8;-khtml-opacity:0.8;opacity:0.8;}
.b-gray{background:rgba(14,14,14,0.1);}
.hidden{display:none;}
.inline-block{display:inline-block;}
.show{display:block;}
.osfixed{position:relative !important;}
.ml10{margin:0 0 0 10px;}
.ml20{margin:0 0 0 20px;}
.mt6{margin-top:6px !important;}
.goods-customer{padding:8px 0}
.disabled{color:#fff !important;background:#ccc !important;}

#popup-simple{position:fixed;left:0;top:0;display:none;width:100%;min-width:320px;height:100%;text-align:center;background:rgba(0, 0, 0, .5);z-index:99999;}
#popup-simple div{position:absolute;left:10%;top:40%;width:80%;background:#fff;-webkit-border-radius:6px;border-radius:6px;}
#popup-simple div p{padding:30px 10px;font-size:14px;}

/* head start */
header{position:fixed;top:0;height:44px;width:100%;min-width:320px;z-index:999;background:#f04444;color:#fff;line-height:44px;}
header h1{font-size:18px;text-align:center;font-weight:normal;}
header .back,.schose-choi-sort-header .back{position:absolute;top:0;left:0;display:block;height:44px;width:44px;background:url(../images/ico-back.png) no-repeat center;-webkit-background-size:28% auto;background-size:28% auto;text-indent:-999em;-webkit-tap-highlight-color:rgba(0,0,0,0);tap-highlight-color:rgba(0,0,0,0);z-index:99;}
header .head-right{position:absolute;top:0;right:0;display:block;height:44px;width:44px;font-size:16px;color:#fff;}
header .head-left{position:absolute;top:0;left:10px;display:block;height:44px;font-size:16px;color:#fff;}
header .x-close{position:absolute;top:0;left:10px;display:block;height:44px;width:14px;background:url(../images/x-close.png) no-repeat center;-webkit-background-size:100% auto;background-size:100% auto;text-indent:-999em;}
/* head end */

/* container start */
.container{width:100%;margin:44px auto 0;outline:none;-webkit-box-sizing:border-box;box-sizing:border-box;}
.wrap{position:relative;margin:0 10px;outline:none;-webkit-box-sizing:border-box;box-sizing:border-box;}
/* container end */

/*accounts start*/
/*accounts login start*/
.login ul{margin:30px 0 0;background:#fff;border-bottom:1px solid #e0e0e0;}
.login li{padding-left:42px;height:57px;-webkit-box-sizing:border-box;box-sizing:border-box;border-top:1px solid #e0e0e0;background:url(../images/ico-phone.png) no-repeat 10px center;-webkit-background-size:auto 42%;background-size:auto 42%;}
.login li:last-child{background:url(../images/ico-password.png) no-repeat 10px center;-webkit-background-size:auto 42%;background-size:auto 42%;}
.login #register-message{background:url(../images/ico-message.png) no-repeat 10px center;-webkit-background-size:auto 42%;background-size:auto 42%;}
.login input{width:100%;padding:20px 0 17px;line-height:18px;font-size:14px;}
.login .btn-red{margin:22px 0;}
.login .r-password{float:left;}
.login .f-password{float:right;}
.login .password a{height:22px;line-height:22px;font-size:14px;color:#000;}
.login .btn-ok{background:url(../images/ico-ok.png) no-repeat left center;-webkit-background-size:auto 100%;background-size:auto 100%;}
/*accounts login end*/
/*register start*/
.register .message{position:relative;background:url(../images/ico-message.png) no-repeat 10px center;-webkit-background-size:auto 42%;background-size:auto 42%;}
.register .message div{position:absolute;top:11px;right:10px;height:34px;padding:0 10px;line-height:34px;color:#fff;background:#f04444;font-size:14px;}
.register .message div.getVerificationCode{color:#f04444;background:#e6e6e6;}
.register .btn-white,.goin-shop{letter-spacing:normal;}
.register-pop-ok{position:absolute;top:0;left:0;width:100%;height:100%;background:rgba(0,0,0,0.5);z-index:9999999;}
.register-pop-ok div{position:absolute;top:50%;left:50%;width:280px;padding:10px 10px 20px;margin:-70px 0 0 -150px;-webkit-border-radius:5px;border-radius:5px;background:#fff;}
.register-pop-ok p{padding:10px 0 20px;text-align:center;line-height:30px;}
.register-pop-ok h4{font-size:22px;text-align:center;line-height:40px;font-weight:normal;}
.register-pop-ok a{margin:0 0 0 6%;width:40%;height:40px;line-height:40px;letter-spacing:normal;}
.register-pop-ok .pop-login{margin:0 6% 0 0;}
#register-already a{width:40%;margin:0 30%;}
#register-already p{line-height:26px;font-size:14px;padding:12px 0 17px;}
#register-psd input{height:40px;width:98%;padding:0 0 0 30px;margin:0 0 20px;font-size:14px;border:1px solid #ddd;background:url(../images/ico-password.png) no-repeat 6px center;-webkit-background-size:auto 60%;background-size:auto 60%;-webkit-box-sizing:border-box;box-sizing:border-box;}
#register-psd a{width:40%;margin:0 30%;}
#unshelf-pop p{padding:20px 0 30px;}
/*register end*/
/*shop start*/
.accounts-shop h2{padding:12px 0 10px;text-align:center;font-size:18px;line-height:20px;}
.accounts-shop p{padding:0 10px;font-size:16px;}
.accounts-shop li{padding:10px;margin:8px 0;font-size:14px;background:#fff;}
.accounts-shop li a{display:block;color:#575757;}
/*shop end*/
/*shiopopen start*/
.shopopen ul{margin:10px 0 10px;background:#fff;border-bottom:1px solid #e0e0e0;}
.shopopen li{height:57px;-webkit-box-sizing:border-box;box-sizing:border-box;border-top:1px solid #e0e0e0;-webkit-background-size:auto 42%;background-size:auto 42%;}
.shopopen span{padding:10px 10px 17px;line-height:18px;font-size:14px;background:#fff;color:#838383;}
.shopopen .ml10 span{padding-left:0;}
.shopopen input{padding:20px 0 17px;width:75%;font-size:14px;}
.shopopen .message{position:relative;-webkit-background-size:auto 42%;background-size:auto 42%;}
.shopopen .message div{position:absolute;top:11px;right:10px;height:34px;padding:0 10px;line-height:34px;color:#fff;background:#d95252;font-size:14px;}
.shopopen .message div.getVerificationCode{color:#f04444;background:#e6e6e6;}
.shopopen .address{display:none;padding:20px 0 17px;width:75%;line-height:18px;font-size:14px;}
.shopopen .line{display:inline;padding:20px 0 17px;width:75%;margin-left:-4px;line-height:18px;font-size:14px;}
.shopopen select, .shop-info .shop-type select{margin:10px 0;width:23%;height:34px;font-size:14px;color:#323232;border:1px solid #ddd;text-indent:4px;line-height:30px;text-align:left;}
.agreement{margin:10px 0 15px;}
.agreement a{color:#323232;}
/*shiopopen end*/
/*accounts end*/

/*wholesale start*/
/*wholesale  index start*/
