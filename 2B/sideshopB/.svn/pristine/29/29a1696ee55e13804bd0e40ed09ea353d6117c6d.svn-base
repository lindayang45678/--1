.order-pay .pay-details,.order-pay .pay-items {border-top:1px solid #e1dede;border-bottom:1px solid #e1dede;margin:10px 0 0;background:#fff;}
.order-pay .pay-details{margin:54px 0 0;}
.order-pay .pay-details .fr{font-size:14px;color:#999;}
.order-pay .pay-details .fr em{color:#f04444;margin:0 0 0 4px;font-size:18px;}
.order-pay .pay-details p,.order-pay .pay-items div{position:relative;display:block;height:44px;line-height:44px;margin:0  0 0 14px;padding:0 14px 0 0;}
.order-pay .pay-details p:first-child,.order-pay .pay-items div{border-bottom:1px solid #e1dede;}
.order-pay .pay-items div:last-child{border:0;}
.order-pay .pay-items img{position:absolute;left:0;top:10px;width:26px;}
.order-pay .pay-details p{text-overflow:ellipsis;white-space:nowrap;overflow:hidden;}
.order-pay button{display:block;height:44px;width:100%;padding:0 30px 0 36px;background:#fff;color:#515151;text-align:left;font-size:14px;line-height:44px;background:url(../images/invoice-off.png) no-repeat right center;-webkit-background-size:auto 60%;background-size:auto 60%;}
.order-pay .btn-red{width:94%;margin:10px 3%;letter-spacing:0;}
.order-pay .paytype-selected button{background:#fff url(../images/invoice-ok.png) no-repeat right center;-webkit-background-size:auto 60%;background-size:auto 60%;}
/* pic-Integration */
/*pifa start */
.order-pay .pay-items span{position:absolute;left:0;top:10px;width:26px;height:26px;background:url(../images/play-all-icon.png) no-repeat;-webkit-background-size:26px auto;background-size:26px auto;}
.pay-items span.lklqb{background-position:0 -30px;}
.pay-items span.lklkj{background-position:0 -61px;}
.pay-items span.wx{background-position:0 -92px;}
.pay-items span.zfb{background-position:0 -122px;}
/*pifa end */
/*order-pay end*/


/* customers start */
.customers .head{height:44px;line-height:44px;text-align:center;background:#fff;}
.customers .head em{margin:0 4px;}
.customers .items span{display:block;line-height:26px;text-indent:10px;}
.customers ul{background:#fff;}
.customers ul li{height:70px;padding:10px;text-indent:12px;border-top:1px solid #eee;}
.customers ul li h4{font-weight:normal;line-height:35px;}
.customers img{width:68px;height:68px;float:left;clear:both;}
.customers p{line-height:35px;}
/* customers end */

#protocol{padding-bottom:10px;background:white;}
.terms{padding:0 10px 20px;color:#9a9a9a;background:#fff;line-heiht:22px;}
.terms span{display:inline-block;text-indent:2em;}
.terms h1{padding-top:8px;text-align:center;font-size:16px;color:#323232;line-height:30px;}
.terms ul li p span{float:left;display:inline-block;width:50%;text-align:left;text-indent:0;}
.terms ul li span del,.terms ul li p.line del{width:100px;}
.terms ul li span em,.terms ul li p.line em{width:40px;}
.terms ul li span em,.terms ul li span del.terms ul li p.line em,.terms ul li p.line del,.terms ul li p i.year{display:inline-block;height:0;border-bottom:1px solid #9A9A9A;}
.terms ul li p.re-indent span{text-indent:-8px;}
.terms ul li p span:first-child{width:60%;}
.terms ul li p span:last-child{width:40%;}
.terms ul li p i.year{width:20px;}
.terms ul li ul.list{width:100%;height:18px;line-height:18px;}
.terms ul li ul.list li{float:left;display:inline-block;width:60px;padding-left:3px;margin-left:2em;}