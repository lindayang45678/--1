/*wholesale  product star*/
.product{padding:10px 0 60px;}
#product-pic{width:100%;height:100%;margin:0 auto;overflow:hidden;}
#product-pic li{width:300px;float:left;}
#product-pic li img{width:100%;}
#pagination{position:relative;bottom: 25px;text-align: center;}
#pagination li{display:inline-block;float:none;text-align:center;overflow:hidden;vertical-align:top;}
#pagination a{display:inline-block;width:10px;height:10px;background:#e0e0e0;margin:0 4px;-webkit-border-radius:8px;border-radius:8px;}
#pagination a.active{background-color:#f04444;}
.product #pro-title,.product #pro-time{padding:0px 10px 10px;margin-top:-10px;line-height:20px;overflow:hidden;font-size:16px;color:#0c0c0c;text-align:center;}
.product #pro-price{padding:6px 0;margin:0 10px;font-size:14px;border-bottom:1px solid #ddd;border-top:1px solid #ddd;}
.product #pro-price div{float:left;width:33.5%;height:40px;line-height:20px;box-sizing:border-box;-moz-box-sizing:border-box;-webkit-box-sizing:border-box;text-align:center;}
.product #pro-price .promo-price i{color:#f04444;;font-size:14px;font-weight:bold;}
.product #pro-price .market-price{width:33%;border-left:1px solid #ddd;border-right:1px solid #ddd;}
.product #pro-price .market-price del{text-decoration:line-through;color:#838383;}
.product #pro-tase div{position:relative;height:34px;margin:10px;-border:1px solid #ddd;font-size:14px;overflow:hidden;}
.product #pro-tase div i{position:absolute;right:0;top:0px;display:inline-block;height:34px;width:34px;background:#fff url(../images/prodetail-down.png) no-repeat center;-webkit-background-size:auto 40%;background-size:auto 40%;text-indent:-999px;border-left:1px solid #ddd;}
.product #pro-tase div select{width:100%;height:34px;font-size:14px;color:#323232;border:1px solid #ddd;text-indent:4px;line-height:30px;text-align:left;}
.product #pro-number{margin:0 10px;font-size:14px;line-height:34px;}
