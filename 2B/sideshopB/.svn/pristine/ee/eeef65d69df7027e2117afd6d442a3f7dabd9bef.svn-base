$(function(){	
	$('.first').find('li').on('tap',function(){
		$('.second').show('slow');
		if($(this).hasClass('color')){
			$(this).removeClass('color');
		}else{
			$(this).addClass('color');
		}
		
	});
	$('.second').find('li').on('tap',function(){
		$('.third').show('slow');
		
	});
	$('.third').find('li').on('tap',function(){
		$('.fourth').show('slow');
		
	});
	
	
})