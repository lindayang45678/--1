package com.lakala.module.wholesale.model;

import java.io.Serializable;

/**
 * Created by HOT.LIU on 2015/2/13.
 */
public class ProductDetailForProductList implements Serializable {

    private static final long serialVersionUID = -4880567695164784682L;

    private GoodsPublishForKDB goods;
    private SkuDetails[] goodsSkuO2OList;

    public GoodsPublishForKDB getGoods() {
        return goods;
    }

    public void setGoods(GoodsPublishForKDB goods) {
        this.goods = (goods == null ? new GoodsPublishForKDB() : goods);
    }

    public SkuDetails[] getGoodsSkuO2OList() {
        return goodsSkuO2OList;
    }

    public void setGoodsSkuO2OList(SkuDetails[] goodsSkuO2OList) {
        this.goodsSkuO2OList = (goodsSkuO2OList == null ? new SkuDetails[0] : goodsSkuO2OList);
    }
}
