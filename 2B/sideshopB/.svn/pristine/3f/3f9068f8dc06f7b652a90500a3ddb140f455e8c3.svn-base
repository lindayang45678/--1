$(function(){
	loadJs();
})

$("#to-pay").on("tap",function(){
	var paychanel = storage.getItem("paychanel") || "";
	var payurl = storage.getItem("payurl") || "";
	cordova._native.pay.pay(
			paychanel,
			payurl,
			function(){
				window.location.href = "paysuccess.html";
			},
			function(){
				window.location.href = "payfail.html";
			}
	)
})