$(function() {
	var liWidth = $('#header-list-slide li').width();
	var listCount = $('#header-list-slide li').length;//获取列表数量
	$("#header-list-slide").css("width", liWidth * listCount + "px");
	var prodetailScroll = new iScroll('header-list', {
		hScroll : true,
		vScroll :false,
		momentum: false,
		snap: true,
		snapSpeed: 400,
	});
});
