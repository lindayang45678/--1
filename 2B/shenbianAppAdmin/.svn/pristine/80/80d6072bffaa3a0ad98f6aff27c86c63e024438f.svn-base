html,body,div,span,applet,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,abbr,acronym,address,big,cite,code,del,dfn,em,img,ins,kbd,q,s,samp,small,strike,strong,sub,sup,tt,var,b,u,i,center,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,input,button,textarea,table,caption,tbody,tfoot,thead,tr,th,td,article,aside,canvas,details,figcaption,figure,footer,header,hgroup,menu,nav,section,summary,time,mark,audio,video{margin:0;padding:0;border:0;}
html,body,div,h1,h2,h3,h4,h5,h6,p,dl,dt,dd,ol,ul,li,fieldset,form,input,button,textarea,table,caption,tbody,tfoot,thead,tr,th,td{outline:none;}
html body{height:100%;width:100%;min-width:320px;margin:0 auto;font:12px/1.5 'Microsoft YaHei',"宋体","arial","tahoma","simsun";color:#323232;background:#f2f2f2;}
article,aside,details,figcaption,figure,footer,header,hgroup,nav,section{display:block;outline:none;}
audio,canvas,video{display:inline-block;*display:inline;*zoom:1;}
audio:not([controls]){display:none;}
ul,ol,li{list-style:none;text-align:left;}
img{height:auto;vertical-align:top;border:0 none;outline:none;}
input,label,button,textarea,select{border:medium none;font-family:"microsoft yahei";}
input[type="submit"],input[type="text"],input[type="button"],input[type="password"]{-webkit-appearance:none;appearance:none;-webkit-tap-highlight-color:rgba(0, 0, 0, 0);-webkit-tap-highlight-color:transparent;line-height:normal;}
input::-webkit-input-placeholder{color:#999;}
input:-moz-placeholder{color:#999;}
input::-moz-placeholder{color:#999;}
input:-ms-input-placeholder{color:#999}
input:focus{outline:0;}
table{border-collapse:collapse;border-spacing:0;}
em,i,s,del,small,u,strike{font-style:normal;font-weight:normal;text-decoration:none;}
a{outline:0;text-decoration:none;color:#bc4040;}
a:active,a:focus{outline:0;}
.color{color:#bc4040;}
.c-white{color:#fff;}
.c-red{color:#f00;}
.c-black{color:#0c0c0c;}
.c-gray{color:#838383;}
.f-10{font-size:10px;}
.f-13{font-size:13px;}
.f-14{font-size:14px;}
.f-16{font-size:16px;}
.f-18{font-size:18px;}
.clear{clear:both;}
.clearfix:before,.clearfix:after{content:' ';display:block;overflow:hidden;visibility:hidden;width:0;height:0;}
.clearfix:after{clear:both;}
.clearfix{zoom:1;}
.fl{float:left;}
.fr{float:right;}
.btn-red{display:block;width:100%;height:48px;line-height:48px;letter-spacing:10px;text-align:center;font-size:16px;-webkit-border-radius:3px;border-radius:3px;color:#fff;background:#d95252;}
.btn-white{display:block;height:44px;line-height:44px;letter-spacing:10px;text-align:center;font-size:16px;-webkit-border-radius:3px;border-radius:3px;color:#d95252;background:#fff;border:2px solid #d95252;}
.btn-gray{display:block;width:100%;height:48px;line-height:48px;letter-spacing:10px;text-align:center;font-size:16px;-webkit-border-radius:3px;border-radius:3px;color:#fff;background:#929292;}
.btn-red:hover,.btn-white:hover,.btn-gray:hover{filter:alpha(opacity=80);-moz-opacity:0.8;-khtml-opacity:0.8;opacity:0.8;}
.hidden{display:none;}
.show{display:block;}
/* head start */
header{position:fixed;top:0;height:44px;width:100%;min-width:320px;z-index:999;background:#bc4040;color:#fff;line-height:44px;}
header h1{font-size:18px;text-align:center;font-weight:normal;}
header .back{position:absolute;top:0;left:10px;display:block;height:44px;width:44px;background:url(../images/ico-back.png) no-repeat left center;-webkit-background-size:30% auto;background-size:30% auto;text-indent:-999px;}
header .head-right{position:absolute;top:0;right:10px;display:block;height:44px;font-size:16px;color:#fff;}
header .head-left{position:absolute;top:0;left:10px;display:block;height:44px;font-size:16px;color:#fff;}
header .x-close{position:absolute;top:0;left:10px;display:block;height:44px;width:14px;background:url(../images/x-close.png) no-repeat center;-webkit-background-size:100% auto;background-size:100% auto;text-indent:-999px;}
/* head end */

/* container start */
.container{position:relative;width:100%;margin:44px auto 0;outline:none;-webkit-box-sizing:border-box;box-sizing:border-box;}
.wrap{position:relative;margin:0 10px;outline:none;-webkit-box-sizing:border-box;box-sizing:border-box;}
/* container end */

/*accounts start*/
/*accounts login start*/
.login ul{margin:30px 0 0;background:#fff;border-bottom:1px solid #e0e0e0;}
.login li{padding-left:42px;height:57px;-webkit-box-sizing:border-box;box-sizing:border-box;border-top:1px solid #e0e0e0;background:url(../images/ico-phone.png) no-repeat 10px center;-webkit-background-size:auto 42%;background-size:auto 42%;}
.login li:last-child{background:url(../images/ico-password.png) no-repeat 10px center;-webkit-background-size:auto 42%;background-size:auto 42%;}
.login input{width:100%;padding:20px 0 17px;line-height:18px;font-size:14px;}
.login .btn-red{margin:22px 0;}
.login .r-password{float:left;}
.login .f-password{float:right;}
.login .password a{height:22px;line-height:22px;padding:0 0 0 33px;font-size:14px;color:#000;}
.login .btn-ok{background:url(../images/ico-ok.png) no-repeat left center;-webkit-background-size:auto 100%;background-size:auto 100%;}
/*accounts login end*/
/*register start*/
.register .message{position:relative;background:url(../images/ico-message.png) no-repeat 10px center;-webkit-background-size:auto 42%;background-size:auto 42%;}
.register .message div{position:absolute;top:11px;right:10px;height:34px;padding:0 10px;line-height:34px;color:#fff;background:#d95252;font-size:14px;}
.register .btn-white{letter-spacing:normal;}
/*register end*/
/*shop start*/
.accounts-shop h2{padding:12px 0 10px;text-align:center;font-size:18px;line-height:20px;}
.accounts-shop p{padding:0 10px;font-size:16px;}
.accounts-shop li{padding:10px;margin:8px 0;font-size:14px;background:#fff;}
.accounts-shop li a{display:block;color:#575757;}
/*shop end*/
/*accounts end*/

/*wholesale start*/
/*wholesale  index start*/
.wholesale-search{height:44px;width:44px;background:url(../images/ico-searchwhite.png) no-repeat right center;-webkit-background-size:auto 44%;background-size:auto 44%;text-indent:-99999px;}
.wholesale{background:#fff;}
#wholesale-l{position:fixed;left:0;top:44px;width:36%;height:100%;border-right:1px solid #ddd;-webkit-box-sizing:border-box;box-sizing:border-box;overflow-x:hidden;overflow-y:auto;}
#menu-l{position:relative;width:100%;height:100%;background:#fff;}
.left-l1 a{position:relative;display:block;height:42px;width:100%;padding:0 10px;-webkit-box-sizing:border-box;box-sizing:border-box;line-height:42px;font-size:16px;overflow:hidden;}
.sj-redr{position:absolute;top:0;right:0;display:block;width:18px;height:42px;background:#fff url(../images/ico-triangle-redr.png) no-repeat 2px center;-webkit-background-size:auto 32%;background-size:auto 32%;}
.left-l1 a.white{background:#fff;}
.left-l1>li{margin-bottom:3px;background:#ececec;}
.left-l2 a{font-size:14px;border-bottom:1px solid #ddd;background:#f8f8f8;color:#323232;}
.left-l3 a{font-size:13px;}
.left-l4 a{text-indent:10px;}
#wholesale-r{float:right;width:64%;padding:10px 6px 70px;-webkit-box-sizing:border-box;box-sizing:border-box;overflow-y:auto;overflow-x:hidden;}
.wholesale-ad{width:100%;height:100%;}
.wholesale-ad li{margin:0 0 6px;border:1px solid #ddd;border-radius:3px;}
.wholesale-ad a{display:block;}
.wholesale-ad a img{width:100%;height:auto;}
.wholesale-goods{width:100%;height:100%;}
.wholesale-goods li{margin:0 0 6px;border:1px solid #ddd;border-radius:3px;}
.wholesale-goods li i{display:block;}
.wholesale-goods li a{display:block;overflow:hidden;}
.wholesale-goods .box-picture{position:relative;float:left;width:104px;height:104px;margin:0 6px 0 0;}
.wholesale-goods .soldout{position:absolute;left:0;top:0;height:104px;width:104px;background:url(../images/ico-soldout.png) no-repeat center;-webkit-background-size:100%;background-size:100%;}
.wholesale-goods .box-picture img{width:auto;height:100%;}
.wholesale-goods .box-detail{position:relative;padding:0 4px 0 0;display:block;overflow:hidden;}
.wholesale-goods .detail-head{max-height:36px;padding:2px 0 0;line-height:18px;overflow:hidden;color:#323232;font-size:14px;}
.wholesale-goods .sale-price{font-size:14px;font-weight:bold;}
.wholesale-goods .market-price{color:#838383;text-decoration:line-through;}
.wholesale-goods .add-cart{margin:0 0 2px;background:#d95252;color:#fff;line-height:24px;text-align:center;font-size:14px;}
#wholesale-foot{position:fixed;bottom:0;left:0;height:62px;width:100%;padding:0 78px 0 66px;-webkit-box-sizing:border-box;box-sizing:border-box;border-top:1px solid #ddd;background:#f2f2f2 url(../images/ico-goodscartred.png) no-repeat 6px center;-webkit-background-size:auto 61%;background-size:auto 61%;}
.goodscart{position:absolute;top:6px;left:32px;display:block;width:22px;height:22px;line-height:20px;text-align:center;border-radius:50%;background:#bc4040;color:#fff;font-size:12px;}
.goodscart-total{font-size:14px;line-height:62px;font-weight:bold;margin: 0 14px 0 0;}
.goodscart-price{font-size:14px;color:#bc4040;line-height:62px;font-weight:bold;}
#wholesale-foot .gopay{position:absolute;right:6px;top:0;height:40px;padding:0 10px;margin-top:11px;line-height:40px;background:#bc4040;color:#fff;font-size:16px;text-align:center;}
#sku{position:fixed;bottom:0;padding:10px;width:100%;-webkit-box-sizing:border-box;box-sizing:border-box;background:#fff;z-index:9999;}
.sku-wrap{max-height:170px;overflow-x:hidden;overflow-y:auto;}
.sku-detail i{font-weight:bold;line-height:30px;}
.sku-detail em{display:inline-block;min-width:40px;padding:0 8px;margin:0 6px 6px 0;color:#838383;font-weight:bold;border:1px solid #d95252;line-height:26px;text-align:center;}
.sku-detail .sku-red{background:#d95252;color:#fff;}
.sku-a i{font-size:12px;display:inline-block;width:50%;overflow:hidden;font-weight:bold;}
.sku-a em{font-weight:bold;}
.sku-market{color:#838383;text-decoration:line-through;}
.sku-b{padding:10px 0;}
.sku-b em{display:inline-block;margin:0 10px 0 0;font-weight:bold;}
.sku-b i{display:inline-block;width:40px;vertical-align:middle;line-height:24px;background:#bc4040;font-size:18px;font-weight:bold;color:#fff;text-align:center;}
.sku-b input{width:70px;font-weight:bold;vertical-align:middle;text-align:center;line-height:22px;border-top:1px solid #bc4040;border-bottom:1px solid #bc4040;}
#sku a{width:47%;font-size:14px;line-height:38px;color:#fff;text-align:center;}
.sku-cancel{margin:6px 6% 0 0;background:#8c8b8b;letter-spacing:10px;}
.sku-sure{margin:6px 0 0;background:#bc4040;}
.masker{position:absolute;top:0;left:0;width:100%;height:100%;padding:999990px 0;background:rgba(0,0,0,0.5);z-index:9999;}
/*wholesale  index end*/

/*wholesale  product star*/
.product{padding:10px 0 60px;}
#product-pic{width:100%;height:100%;margin:0 -auto;overflow:hidden;}
#product-pic li{width:300px;float:left;}
#product-pic li img{width:300px;}
#pagination{position:relative;bottom: 25px;text-align: center;}
#pagination li{display:inline-block;float:none;text-align:center;overflow:hidden;vertical-align:top;}
#pagination a{display:inline-block;width:10px;height:10px;background:#e0e0e0;margin:0 4px;-webkit-border-radius:8px;border-radius:8px;}
#pagination a.active{background-color:#bc4040;}
.pro-title{padding:10px 0 0;font-size:14px;font-weight:bold;}
.pro-content div{padding:6px 0;font-size:14px;}
.pro-content .pro-lakala{padding:0;}
.pro-lakala i{font-size:14px;color:#bc4040;}
.pro-btn{position:fixed;bottom:0;left:0;width:100%;padding:0 10px 6px;-webkit-box-sizing:border-box;box-sizing:border-box;background:#f2f2f2;}
.product .pro-btn a{width:47%;font-size:14px;line-height:38px;color:#fff;text-align:center;}
/*wholesale  product end*/

/*wholesale search star*/
.search-wrap{position:relative;padding:10px 0 0;}
.search-wrap div{padding:11px 0 9px 36px;border:1px solid #ddd;background:#fff url(../images/search-img.png) no-repeat 10px;-webkit-background-size:auto 50%;background-size:auto 50%;}
.search-wrap input{width:100%;height:16px;line-height:16px;font-size:14px;}
.w-searchbtn{position:absolute;top:11px;right:1px;display:block;width:60px;line-height:39px;text-align:center;color:#fff;font-size:14px;background:#bc4040;}
.w-search-failimg{margin:20px 35%;width:30%;}
.w-search-fail{font-size:16px;text-align:center;}
.search-result{float:none;padding:0 0 70px;overflow-x:hidden;overflow-y:auto;}
.search-result li{background:#fff;}
.search-result .box-picture{height:130px;width:130px;}
.search-result .soldout{width:130px;height:130px;}
.search-title{padding:6px 0;font-size:14px;}
.search-title em{color:#bc4040;}
.search-result .detail-head{max-height:40px;font-size:16px;line-height:20px;}
.search-result .sale-price{font-size:16px;}
.search-result .market-price{font-size:14px;}
.search-result .add-cart{margin-top:6px;font-size:16px;line-height:30px;}
/*wholesale search end*/

/*wholesale fill start*/
.fill-head{padding:8px 0 0;border-bottom:1px solid #ddd;}
.fill-head li{position:relative;padding:14px 10px;background:#fff;border-top:1px solid #ddd;}
.fill-head li:nth-child(1){padding:0 10px;font-weight:bold;}
.fill-head li:nth-child(1) div{padding:4px 0;}
.fill-head li:nth-child(1) i{display:inline-block;margin:0 10px 0 0;line-height:18px;font-weight:bold;}
.fill-head li a{display:block;}
.fill-head li em{display:inline-block;padding:0 4px 0 0;font-size:14px;color:#0c0c0c;font-weight:bold;}
.choose-other{position:absolute;top:4px;right:10px;display:inline-block;padding:0 6px;background:#bc4040;font-weight:normal;font-size:12px;line-height:30px;color:#fff;}
.fill-total{display: inline-block;width:99%;line-height:16px;vertical-align:middle;overflow:hidden;}
.fill-middle{display: inline-block;height:62px;width:1%;vertical-align:middle;}
.fill-head .btn-circle{display:inline-block;height:20px;padding:0 0 0 20px;background:url(../images/red-circle.png) no-repeat left center;-webkit-background-size:auto 80%;background-size:auto 80%;}
.fill-head .r-arrow{position:absolute;right:14px;top:15px;display:block;width:10px;height:20px;background:url(../images/right-icon.png) no-repeat left center;-webkit-background-size:auto 80%;background-size:auto 80%;}
.fill-head .hidden{display:none;}
.fill-head .fill-other{position:absolute;right:28px;top:14px;}
.fill-coupons input{padding:0 0 0 4px;width:170px;line-height:28px;font-size:12px;font-weight:bold;border:1px solid #ddd;}
.fill-coupons i{display:inline-block;padding:8px 0 0 74px;color:#838383;}
.fill-coupons-cancel{position:absolute;right:10px;top:17px;display:inline-block;padding:0 4px;font-size:16px;line-height:24px;color:#fff;background:#bc4040;}
.fill-invoicem{position:absolute;left:10px;top:14px;}
.fill-invoice{display:block;padding:0 30px 0 90px;font-size:14px;color:#0c0c0c;font-weight:bold;}
.fill-cart{padding:10px 10px 0 10px;font-size:14px;}
.fill-cart i{font-weight:bold;}
.fill-content{background:#fff;margin-bottom:70px;}
.fill-content dl{padding:0 10px 0;border-top:1px solid #ddd;}
.fill-content dt{padding:4px 0;line-height:18px;}
.fill-content dd{position:relative;padding:0 0 0 120px;}
.fill-content dd div{font-size:14px;}
.fill-content .fill-pic{position:absolute;top:0px;left:0px;}
.fill-content .fill-pic img{width:80px;height:80px;border:1px solid #ddd;margin:0 10px 0 30px;}
.fill-sku i{display:inline-block;margin-right:14px;}
.fill-close{position:absolute;top:30px;left:0px;height:24px;width:24px;background:url(../images/redius-image.png) no-repeat center;-webkit-background-size:auto 100%;background-size:auto 100%;}
.fill-change{float:right;padding:10px 0;}
.fill-change i{display:inline-block;padding:0 10px;vertical-align:middle;font-weight:bold;font-size:18px;line-height:22px;color:#fff;background:#bc4040;}
.fill-change input{width:60px;vertical-align:middle;text-align:center;border:1px solid #bc4040;line-height:20px;font-size:14px;font-weight:bold;}
.fill-pro-title{font-weight:bold;}
/*wholesale fill end*/

/*wholesale fill address start*/
.fill-address{padding:10px 0 0;}
.fill-address li{position:relative;padding:0 10px 10px 40px;margin-bottom:10px;background:#fff;border-top:1px solid #ddd;border-bottom:1px solid #ddd;}
.fill-address li i{line-height:18px;}
.fill-address li div{padding:6px 0 0;}
.fill-edit{position:absolute;top:8px;right:10px;display:inline-block;padding:0 8px;border-top:1px solid #ddd;border-bottom:1px solid #ddd;font-size:14px;line-height:24px;color:#fff;background:#bc4040;}
.fill-address .fill-close{position:absolute;left:10px;padding:0;}
.fill-add-address{line-height:50px;font-size:14px;color:#bc4040;background:#fff;text-align:center;border-top:1px solid #ddd;border-bottom:1px solid #ddd;}
.fill-edit-address li{position:relative;padding: 20px 10px;font-size:14px;-webkit-box-sizing:border-box;box-sizing:border-box;border-top:1px solid #ddd;background:#fff;}
.fill-edit-address li i{position:absolute;left:10px;}
.fill-edit-address li input{width:100%;padding:0 0 0 70px;line-height:20px;-webkit-box-sizing:border-box;box-sizing:border-box;}
.fill-edit-address .edit-name input{padding-left:86px;}
.fill-edit-address .edit-phone input{padding-left:110px;}
.fill-edit-address .save-address{-webkit-box-sizing:border-box;box-sizing:border-box;}
.popup{position:fixed;top:50%;width:89%;min-height:150px;padding:20px 10px;;margin:-40% 5% 0;-webkit-box-sizing:border-box;box-sizing:border-box;background:#fff;z-index:99999;border-radius:6px;}
.popup p{color:#bc4040;vertical-align:middle;font-size:14px;text-align:center;line-height:40px;}
.popup .popup-btn{width:24%;margin:10px 38% 0;height:30px;line-height:30px;text-align:center;letter-spacing:normal;}


/*wholesale fill address end*/

/*wholesale fill paytype star*/
.paytype-body{background:#fff;}
.paytype{height:100%;padding:20px 10px 0;background:#fff;}
.paytype button{display:block;height:30px;padding:0 0 0 40px;line-height:30px;font-size:15px;font-weight:bold;background:url(../images/un-on.png) no-repeat 2px center;-webkit-background-size:auto 60%;background-size:auto 60%;}
.paytype-sure{margin-top:30px;}
.paytype .paytype-selected{background:url(../images/red-circle.png) no-repeat 2px center;-webkit-background-size:auto 60%;background-size:auto 60%;}
/*wholesale fill paytype end*/

/*wholesale fill coupon end*/
.fill-coupon-tab{padding:10px 10px 0;background:#f5f5f5;border-bottom:1px solid #ddd;}
.fill-coupon-tab span{display:inline-block;width:50%;line-height:30px;text-align:center;font-size:16px;}
.fill-coupon-tab span em{border-bottom:2px solid #f5f5f5;}
.fill-coupon-tab .tab-red{border-bottom:2px solid #bc4040;}
.select-coupon{margin:10px 0 0;}
.select-coupon li{position:relative;padding:10px 80px 10px 30px;margin:0 0 10px;background:#fff url(../images/un-on.png) no-repeat 10px center;-webkit-background-size:auto 30%;background-size:auto 30%;border-bottom:1px solid #ddd;border-top:1px solid #ddd;}
.select-coupon .coupon-on{background:#fff url(../images/ico-ok.png) no-repeat 10px center;-webkit-background-size:auto 30%;background-size:auto 30%;}
.select-coupon .coupon-off-gray{background:#fff url(../images/ico-off-gray.png) no-repeat 10px center;-webkit-background-size:auto 30%;background-size:auto 30%;}
.coupon-change{position:absolute;right:10px;top:1px;height:18px;padding:19px 0;}
.coupon-change i{display:inline-block;width:16px;vertical-align:middle;line-height:16px;border-radius:50%;font-size:14px;background:#bc4040;color:#fff;text-align:center;}
.coupon-change input{width:30px;vertical-align:middle;line-height:18px;font-size:16px;text-align:center;}
.coupon-pa{margin:6px 0 20px 0;}
.fill-coupon-use{position:relative;padding:0 40px 0 80px;margin:10px 0;background:#fff;border-bottom:1px solid #ddd;border-top:1px solid #ddd;}
.coupon-use-text{position:absolute;left:10px;top:1px;font-size:14px;font-weight:bold;line-height:54px;}
.fill-coupon-use input{width:100%;line-height:16px;padding:20px 0;}
.coupon-use-x{position:absolute;top:1px;right:10px;height:54px;width:20px;background:url(../images/counpon-use-x.png) no-repeat center;-webkit-background-size:100%;background-size:100%;}
.fill-coupon-use .hidden{display:none;}
.test-ok{margin:0 0 10px;text-align:center;}
.test-fail{margin:0 0 10px;text-align:center;white-space:nowrap;text-overflow:hidden;}
.test-used{margin:0 0 10px;text-align:center;white-space:nowrap;text-overflow:hidden;}
/*wholesale fill coupon start*/

/*wholesale fill invoice start*/
.invoice-index{padding:10px 10px 20px 10px;margin:60px 0 20px;background:#fff;font-size:14px;}
.invoice-index li{position:relative;padding:10px 30px 10px 80px;}
.invoice-index li i{position:absolute;left:0;top:14px;}
