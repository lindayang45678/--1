html,body,div,span,applet,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,abbr,acronym,address,big,cite,code,del,dfn,em,img,ins,kbd,q,s,samp,small,strike,strong,sub,sup,tt,var,b,u,i,center,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,input,button,textarea,table,caption,tbody,tfoot,thead,tr,th,td,article,aside,canvas,details,figcaption,figure,footer,header,hgroup,menu,nav,section,summary,time,mark,audio,video{margin:0;padding:0;border:0;}
html,body,div,h1,h2,h3,h4,h5,h6,p,dl,dt,dd,ol,ul,li,fieldset,form,input,button,textarea,table,caption,tbody,tfoot,thead,tr,th,td{outline:none;}
html body{height:100%;width:100%;min-width:320px;margin:0 auto;font:12px/1.5 'Microsoft YaHei',"宋体","arial","tahoma","simsun";color:#323232;background:#f2f2f2;}
article,aside,details,figcaption,figure,footer,header,hgroup,nav,section{display:block;outline:none;}
audio,canvas,video{display:inline-block;*display:inline;*zoom:1;}
audio:not([controls]){display:none;}
ul,ol,li{list-style:none;text-align:left;}
img{height:auto;vertical-align:top;border:0 none;outline:none;}
input,label,button,textarea,select{border:medium none;font-family:"microsoft yahei";}
input[type="submit"],input[type="text"],input[type="button"],input[type="password"]{-webkit-appearance:none;appearance:none;-webkit-tap-highlight-color:rgba(0, 0, 0, 0);-webkit-tap-highlight-color:transparent;line-height:normal;}
input::-webkit-input-placeholder{color:#999;}
input:-moz-placeholder{color:#999;}
input::-moz-placeholder{color:#999;}
input:-ms-input-placeholder{color:#999}
input:focus{outline:0;}
table{border-collapse:collapse;border-spacing:0;}
em,i,s,del,small,u,strike{font-style:normal;font-weight:normal;text-decoration:none;}
a{outline:0;text-decoration:none;color:#bc4040;}
a:active,a:focus{outline:0;}
.color{color:#bc4040;}
.c-white{color:#fff;}
.c-red{color:#f00;}
.c-black{color:#0c0c0c;}
.c-gray{color:#838383;}
.f-10{font-size:10px;}
.f-13{font-size:13px;}
.f-14{font-size:14px;}
.f-16{font-size:16px;}
.f-18{font-size:18px;}
.clear{clear:both;}
.clearfix:before,.clearfix:after{content:' ';display:block;overflow:hidden;visibility:hidden;width:0;height:0;}
.clearfix:after{clear:both;}
.clearfix{zoom:1;}
.fl{float:left;}
.fr{float:right;}
.hidden{display:none;}
.show{display:block;}
.btn-red{display:block;width:100%;height:48px;line-height:48px;letter-spacing:10px;text-align:center;font-size:16px;-webkit-border-radius:3px;border-radius:3px;color:#fff;background:#d95252;}
.btn-white{display:block;height:44px;line-height:44px;letter-spacing:10px;text-align:center;font-size:16px;-webkit-border-radius:3px;border-radius:3px;color:#d95252;background:#fff;border:2px solid #d95252;}
.btn-gray{display:block;width:100%;height:48px;line-height:48px;letter-spacing:10px;text-align:center;font-size:16px;-webkit-border-radius:3px;border-radius:3px;color:#fff;background:#929292;}
.btn-red:hover,.btn-white:hover,.btn-gray:hover{filter:alpha(opacity=80);-moz-opacity:0.8;-khtml-opacity:0.8;opacity:0.8;}

/* head start */
header{position:fixed;top:0;height:44px;width:100%;z-index:999;background:#bc4040;color:#fff;line-height:44px;}
header h1{font-size:18px;text-align:center;font-weight:normal;}
header .back{position:absolute;top:0;left:10px;display:block;height:44px;width:10px;background:url(../images/ico-back.png) no-repeat center/100% auto;text-indent:-999px;}
header .head-right{position:absolute;top:0;right:10px;display:block;height:44px;font-size:16px;color:#fff;}
header .head-left{position:absolute;top:0;left:10px;display:block;height:44px;font-size:16px;color:#fff;}
/* head end */

/* container start */
.container{position:relative;width:100%;margin:44px auto 0;outline:none;-webkit-box-sizing:border-box;box-sizing:border-box;}
.wrap{position:relative;margin:0 10px;outline:none;-webkit-box-sizing:border-box;box-sizing:border-box;}
/* container end */

/*accounts start*/
/*accounts login start*/
.login ul{margin:30px 0 0;background:#fff;border-bottom:1px solid #e0e0e0;}
.login li{padding-left:42px;height:57px;-webkit-box-sizing:border-box;box-sizing:border-box;border-top:1px solid #e0e0e0;background:url(../images/ico-phone.png) no-repeat 10px center/auto 42%;}
.login li:last-child{background:url(../images/ico-password.png) no-repeat 10px center/auto 42%;}
.login input{width:100%;padding:20px 0 17px;line-height:18px;font-size:14px;}
.login .btn-red{margin:22px 0;}
.login .r-password{float:left;}
.login .f-password{float:right;}
.login .password a{height:22px;line-height:22px;padding:0 0 0 33px;font-size:14px;color:#000;}
.login .btn-ok{background:url(../images/ico-ok.png) no-repeat left center/auto 100%;}
/*accounts login end*/
/*register start*/
.register .message{position:relative;background:url(../images/ico-message.png) no-repeat 10px center/auto 42%;}
.register .message div{position:absolute;top:11px;right:10px;height:34px;padding:0 10px;line-height:34px;color:#fff;background:#d95252;font-size:14px;}
.register .btn-white{letter-spacing:normal;}
/*register end*/
/*shop start*/
.accounts-shop h2{padding:12px 0 10px;text-align:center;font-size:18px;line-height:20px;}
.accounts-shop p{padding:0 10px;font-size:16px;}
.accounts-shop li{padding:10px;margin:8px 0;font-size:14px;background:#fff;}
.accounts-shop li a{display:block;color:#575757;}
/*shop end*/
/*accounts end*/

/*wholesale start*/
/*wholesale  index start*/
.wholesale-search{height:44px;width:44px;background:url(../images/ico-searchwhite.png) no-repeat right center/auto 44%;text-indent:-99999px;}
.wholesale{background:#fff;}
#wholesale-l{position:fixed;left:0;top:44px;width:36%;height:100%;border-right:1px solid #ddd;-webkit-box-sizing:border-box;box-sizing:border-box;overflow-x:hidden;overflow-y:auto;}
#menu-l{position:relative;width:100%;height:100%;background:#fff;}
.left-l1 a{position:relative;display:block;height:42px;width:100%;padding:0 10px;-webkit-box-sizing:border-box;box-sizing:border-box;line-height:42px;font-size:16px;overflow:hidden;}
.sj-redr{position:absolute;top:0;right:0;display:block;width:18px;height:42px;background:#fff url(../images/ico-triangle-redr.png) no-repeat 2px center/auto 32%;}
.left-l1 a.white{background:#fff;}
.left-l1>li{margin-bottom:3px;background:#ececec;}
.left-l2 a{font-size:14px;border-bottom:1px solid #ddd;background:#f8f8f8;color:#323232;}
#wholesale-r{float:right;width:64%;padding:10px 6px 0;-webkit-box-sizing:border-box;box-sizing:border-box;overflow-y:auto;overflow-x:hidden;}
.wholesale-ad li{margin:0 0 6px;border:1px solid #ddd;border-radius:3px;}
.wholesale-ad a{display:block;}
.wholesale-ad a img{width:100%;height:auto;}
#wholesale-foot{position:fixed;bottom:0;left:0;height:62px;width:100%;padding:0 6px 0 66px;-webkit-box-sizing:border-box;box-sizing:border-box;boder-top:1px solid #ddd;background:#f2f2f2 url(../images/ico-goodscartred.png) no-repeat 6px center/auto 61%;}
#goodscart{position:absolute;top:6px;left:32px;display:block;width:22px;height:22px;line-height:20px;text-align:center;border-radius:50%;background:#bc4040;color:#fff;font-size:12px;}
#goodscart-total{font-size:14px;line-height:62px;font-weight:bold;}
#goodscart-price{font-size:14px;color:#bc4040;line-height:62px;font-weight:bold;}
#wholesale-foot .gopay{float:right;height:40px;padding:0 10px;margin-top:11px;line-height:40px;background:#bc4040;color:#fff;font-size:16px;text-align:center;}
/*wholesale  index end*/
/*wholesaleend*/

/*homepage  start*/
.homepage header span{font-size:10px;vertical-align:top;line-height:40px;margin-left:2px;}
.homepage .head-left{float:left;display:block;height:44px;font-size:16px;color:#fff;background:url(../images/icon-tz.png) no-repeat left center;}
.homepage .head-left em{float:right;width:14px;height:14px;font-size:8px;line-height:14px;text-align:center;color:#bc4040;margin-top:4px;border-radius:50%;padding:1px;background-color:#fff;}
.homepage .head-right{float:right;display:block;line-height:44px;font-size:16px;color:#fff;background:url(../images/icon-sz.png) no-repeat left center;}
.homepage .head-left,.homepage .head-right{width:30px;-webkit-background-size:100%;background-size:100%;}
.homepage-main table{width:100%;margin-top:20px;}
.homepage-main table td{position:relative;width:46%;-webkit-box-sizing:border-box;box-sizing:border-box;}
.homepage-main table td a{display:block;}
.homepage-main table td img{width:96%;margin:2%;-webkit-box-sizing:border-box;box-sizing:border-box;}
.homepage-main table td h3{position:absolute;bottom:20%;left:0;width:100%;text-align:center;color:#fff;font-size:16px;}
.homepage .prev,.homepage .next{display:block;width:36px;height:36px;z-index:9999;}
.homepage .prev{position:fixed;top:50%;left:10px;background:url(../images/ico-btn-left.png) no-repeat left center;}
.homepage .next{position:fixed;top:50%;right:10px;background:url(../images/ico-btn-right.png) no-repeat right center;}
.homepage ul{position:fixed;bottom:10%;left:0;width:100%;text-align:center;z-index:9999;}
.homepage ul li{display:inline-block;width:18px;height:18px;margin-right:4px;border-radius:50%;background:#ccc;}
.homepage ul li.active{background-color:#cc6666;}
/* homepage  end*/


/*notice start*/
.notice .head-left{float:left;display:block;width:30px;font-size:16px;color:#fff;background:url(../images/ico-back.png) no-repeat left center;-webkit-background-size:50%;background-size:50%;}
.notice-main{background:#eee;}
.notice .products{display:block;width:92%;margin:10px auto 1px;-webkit-box-shadow:1px 1px 3px;box-shadow:1px 1px 3px #ccc;padding:2%;}
.notice .products div{position:relative;}
.notice .products .top div{position:absolute;right:0;bottom:0;width:100%;height:26px;line-height:26px;font-size:18px;color:#fff;background: rgba(0, 0, 0,0.5);text-indent:2px;overflow:hidden;}
.notice .products img{width:100%;} 
.notice-wrap h4{line-height:36px;font-size:18px;font-weight:normal;text-align:center;color:#fff;background:#eee;}
.notice-wrap h4 span{line-height:26px;font-size:18px;color:#fff;background: rgba(204, 204, 204,0.5);padding:0 20px;border-radius:4px;}
.notice .items{padding-top:10px;}
.notice .items p{float:left;width:82%;height:52px;line-height:26px;font-size:18px;color:#000;overflow:hidden;}
.notice .items img{width:18%;}
/*notice end*/
/*setting start*/
.setting .head-left{float:left;display:block;width:30px;font-size:16px;color:#fff;background:url(../images/ico-back.png) no-repeat left center;-webkit-background-size:50%;background-size:50%;}
.setting-main p{padding:2px 10px;font-size:14px;background:#eee;border-bottom:1px solid #d4Dfe4;}
.setting-choose{border-bottom:1px solid #d4Dfe4;background-color:#fff;padding:10px;}
.setting-choose h4{display:inline-block;padding-top:6px;font-size:16px;color:#000;}
.setting-choose div{position:relative;width:54px;height:30px;border-radius:30px;background-color:#e4e4e4;}
.setting-choose div a{display:inline-block;width:28px;height:28px;background-color:#fff;border-radius:14px;margin:1px 0 0 1px;position:absolute;left:1px;top:0px;-webkit-transition:all linear .2s;-moz-transition:all linear .2s;-ms-transition:all linear .2s;transition:all 0.2s linear 0s;}
.setting-choose .setting-choose-on{background-color:#58DB77;}/*开*/
.setting-choose .setting-choose-on-slide{position:absolute;left:24px;top:0px;-webkit-transition:all linear .2s;-moz-transition:all linear .2s;-ms-transition:all linear .2s;transition:all 0.2s linear 0s;}/*开*/
.paytype-status-off{background:url(../images/ico-arrow-off.png) no-repeat right center;padding-right:28px;background-size:20px auto;}
.paytype-status-on{background:url(../images/ico-arrow-on.png) no-repeat right center;background-size:20px auto;}
.setting-choose-top{margin-top:8px;}
/*setting end*/

/* details-main start */
.details .head-left{float:left;display:block;width:30px;font-size:16px;color:#fff;background:url(../images/ico-back.png) no-repeat left center;-webkit-background-size:50%;background-size:50%;}
.details-main{color:#000;}
.details-main img{width:100%;margin:6px auto;}
.details-main h3{font-size:18px;font-weight:normal;margin-top:8px;}
.details-main .time{color:#999;}
/* details-main end */

/* shops  start */
.nobr,.searchyes-list dl dd h3,.searchyes-list dl dd p,.searchyes-list dl dd del,.shopindex-part1 a div h2,.shopindex-list a h3,.change-tagname{overflow:hidden;white-space:nowrap;text-overflow:ellipsis;}
/*searchgoods  start*/
.goods-search{position:relative;width:100%;height:36px;margin:10px 0;border:1px solid #e1dede;box-sizing:border-box;background-color:#fff;}
.noresult{margin-bottom:45px;}
.goods-search input{width:100%;height:34px;padding:10px 20% 10px 40px;box-sizing:border-box;-webkit-background-image:url(../images/search-img.png);-webkit-background-position:10px center;-webkit-background-size:22px 22px;background-image:url(../images/search-img.png);background-position:10px center;background-size:22px 22px;vertical-align:middle;}
.goods-search button{position:absolute;right:1px;top:1px;width:18%;height:32px;background:#c35d5d;color:#fff;text-align:center;ling-height:34px;font-size:14px;}
/*searchgoods  end*/
/* searchno  start */
.noresult-img{height:104px;margin:0 auto 30px;-webkit-background-image:url(../images/searchno-img.png);-webkit-background-size:auto 100%;-webkit-background-position:center center;background-image:url(../images/searchno-img.png);background-size:auto 100%;background-position:center center;}
.noresult-font,.search-font{width:100%;height:24px;text-align:center;font-size:16px;}
/* searchno  end */
/* searchyes  start */
.search-font{margin-bottom:8px;}
.yesresult{margin-bottom:9px;}
.searchyes-list{width:100%;margin-bottom:10px;background:#fff;border-top:1px solid #e0e0e0;border-bottom:1px solid #e0e0e0;box-shadow:0 0 5px #ebebed;box-sizing:border-box;overflow:hidden;}
.searchyes-list h2{width:100%;height:44px;margin-bottom:15px;padding-left:10.5%;line-height:44px;box-sizing:border-box;font-size:16px;font-weight:normal;border-bottom:1px solid #ebebeb;-webkit-background-image:url(../images/platform-goods.png);-webkit-background-position:3.3% center;-webkit-background-size:24px;background-image:url(../images/platform-goods.png);background-position:3.3333% center;background-size:24px;}
.selfgoods h2{-webkit-background-image:url(../images/selfrun-goods.png);background-image:url(../images/selfrun-goods.png);}
.searchyes-list dl{width:95%;margin:0 auto;padding-bottom:15px;overflow:hidden;}
.searchyes-list dl dt{float:left;width:20%;margin-right:2.1%;}
.searchyes-list dl dt img{width:100%;height:auto;vertical-align:top;}
.searchyes-list dl dd{float:left;width:74%;}
.searchyes-list dl dd h3{height:20px;line-height:20px;font-size:14px;}
.searchyes-list dl dd p{height:26px;line-height:26px;}
.searchyes-list dl dd del{height:12px;line-height:12px; text-decoration:line-through;}
.searchyes-list dl dd h3,.searchyes-list dl dd p,.searchyes-list dl dd del{width:100%;}
.searchyes-list dl dd del{color:#a8a8a8;}
/* searchyes  end */
/*shopsindex  start*/
.shopindex-part1{width:100%;height:99px;margin-bottom:4px;}
.shopindex-part1 a{display:block;height:99px;}
.shopindex-part1 a div{height:89px;padding:10px 0 0 26%;-webkit-background-image:url(../images/shopindex-img.png);-webkit-background-position:4.1% center;-webkit-background-size:52px 52px;background-image:url(../images/shopindex-img.png);background-position:4.1% center;background-size:52px 52px;}
.shopindex-part1 a div h2{width:86%;height:40px;line-height:40px;font-size:16px;font-weight:normal;color:#323232;}
.shopindex-part1 a div span{display:inline-block;padding-left:20px;line-height:22px;-webkit-background-image:url(../images/share.png);-webkit-background-position:left center;-webkit-background-size:13px 18px;background-image:url(../images/share.png);background-position:left center;background-size:13px 18px;color:#838383;}
.shopindex-list,.shopindex-part1 a{width:100%;background-color:#fff;-webkit-background-image:url(../images/right-icon.png);-webkit-background-position:93.33% center;-webkit-background-size:9px 16px;background-image:url(../images/right-icon.png);background-position:93.33% center;background-size:9px 16px;}
.shopindex-list{height:40px;}
.shopindex-list a{display:block;width:100%;}
.shopindex-list a h3{width:90%;height:40px;padding-left:13.6%;line-height:40px;box-sizing:border-box;color:#838383;font-size:14px;-webkit-background-image:url(../images/shopindex-1.png);-webkit-background-position:5.86% center;-webkit-background-size:16px 16px;background-image:url(../images/shopindex-1.png);background-position:5.86% center;background-size:16px 16px;}
.shopindex-list.list2{border-bottom:1px solid #cacaca;}
.shopindex-list.list2 a h3{-webkit-background-image:url(../images/shopindex-2.png);background-image:url(../images/shopindex-2.png);}
.shopindex-list.list3 a h3{-webkit-background-image:url(../images/shopindex-3.png);background-image:url(../images/shopindex-3.png);}
.add-goods{width:100%;padding:57px 0;box-sizing:border-box;background-color:#e2e2e2;overflow:hidden;text-align:center;}
.add-goods a{display:inline-block;width:12.5%;}
.add-goods a img{width:100%;margin-bottom:17px;}
.add-goods p{width:66%;margin:0 auto;line-height:20px;font-size:14px;color:#838383;}
.goods-search input,.noresult-img,.searchyes-list h2,.shopindex-part1 a div,.shopindex-part1 a div span,.shopindex-list,.shopindex-part1 a,.shopindex-list a h3,.edit-line1 a,.add-service{-webkit-background-repeat:no-repeat;background-repeat:no-repeat;}
/*shopsindex  end*/
/*editshop  start*/
.editshop{margin-top:50px;}
.shopedit-3item{margin-bottom:9px;}
.editline-n,.shopedit-2item{box-shadow:0 1px 3px #e1e1e1;}
.shopedit-2item,.shopedit-3item{background-color:#fff;overflow:hidden;border-top:1px solid #e0e0e0;}
.edit-line1{max-height:52px;line-height:20px;overflow:hidden;}
.editline-n,.edit-line1{font-size:14px;}
.edit-line1 span,.editline-n span.left{float:left;display:inline-block;width:31%;max-height:52px;padding:16px 0;box-sizing:border-box;text-align:center;color:#838383;border-bottom:1px solid #e0e0e0;}
.shopedit-2item.pright-per7{margin-bottom:9px;}
.edit-line1 div,.shopedit-2item.pright-per7 p{float:left;width:69%;max-height:52px;box-sizing:border-box;border-left:1px solid #e0e0e0;border-bottom:1px solid #e0e0e0;}
.change-tagname{float:left;display:inline-block;width:75%;max-height:52px;margin-right:1%;padding:16px 0 16px 8.5%;box-sizing:border-box;}
.shopedit-2item.pright-per7 p{padding-right:4%;}
.edit-line1 a,.editline-n div div a{float:right;display:inline-block;width:20px;height:20px;margin:15px 6% 0 0;background-image:url(../images/edit-icon.png);background-position:left center;background-size:20px 20px;}
.editline-n{position:relative;height:auto;}
.editline-n div.leftborder{width:69%;margin-left:31%;border-left:1px solid #e0e0e0;box-sizing:border-box;}
.line-n{width:100%;padding:16px 9% 12px 5%;line-height:20px;white-space:normal;box-sizing:border-box;}
.editline-n span.left{position:absolute;top:50%;min-height:80px;max-height:none;margin-top:-40px;border:none;line-height:48px;}
.editline-n div div{width:100%;height:32px;line-height:32px;border-top:1px solid #f1f1f1;box-sizing:border-box;background:#fcfcfc;}
.editline-n div div span{float:left;display:inline-block;width:76%;text-align:right;}
.editline-n div div a{float:right;margin:4px 6% 0;}
.add-service{display:inline-block;width:100%;height:45px;background-color:#f2f2f2;background-image:url(../images/addred-icon.png);background-position:5.1% center;background-size:28px 28px;line-height:45px;}
.add-service p{margin-left:15.8%;color:#838383;}
.shopedit-2item.pright-per7 p{margin:0;}
.eddit-line2{position:relative;width:100%;max-height:72px;line-height:20px;border-bottom:1px solid #e0e0e0}
.eddit-line2 span,.eddit-line2 p.change-tagname,.eddit-line2 input.change-tagname{float:left;display:inline-block;min-height:52px;max-height:72px;line-height:20px;}
.eddit-line2 span{position:absolute;float:none;top:50%;min-height:52px;max-height:72px;margin-top:-26px;border-bottom:none;}
.eddit-line2 p.change-tagname,.eddit-line2 input.change-tagname{width:69%;padding-right:7%;box-sizing:border-box;white-space:normal;text-overflow:clip;}
.eddit-line2 p.change-tagname{margin-left:31%;border-bottom:none;}
.upload-panels{display:inline-block;width:100%;height:58px;margin-bottom:10px;line-height:58px;border-top:1px solid #fff;border-bottom:1px solid #fff;background-color:#b9b9b9;-webkit-background-image:url(../images/upload-icon.png);-webkit-background-repeat:no-repeat;-webkit-background-position:24.8% center;-webkit-background-size:39px 39px;background-image:url(../images/upload-icon.png);background-repeat:no-repeat;background-position:24.8% center;background-size:39px 39px;}
.upload-panels p{width:100%;padding-left:40.5%;box-sizing:border-box;font-size:16px;color:#fff;}
.editshop-bigimg{width:100%;height:120px;}
.editshop-bigimg img{width:100%;}
/*editshop  end*/
/*nonstock  start*/
.nonstock-part1{padding:16px 2.4% 20px 2.4%;box-sizing:border-box;}
.nonstock-part1,.nonstock-part2{width:100%;margin-bottom:12px;background:#fff;border-bottom:3px solid #e0e0e0;box-shadow:0 1px 2px #e9e9e9;}
.nonstock-part1 ul{overflow:hidden;text-align:center;}
.nonstock-part1 ul li{position:relative;display:inline-block;width:20%;margin:0 1.6%;}
.nonstock-part1 ul li a{display:inline-block;position:absolute;width:24%;height:24%;right:2px;top:2px;background-image:url(../images/redius-image.png);background-repeat:no-repeat;background-position:center;background-size:100% auto;}
.nonstock-part1 ul li:first-child a{position:static;width:100%;height:100%;background:none;}
.nonstock-part1 ul li del{position:absolute;}
.nonstock-part1 ul li img{width:100%;}

/*nonstock  end*/
/* shops  end */

/*receipt start*/
.receipt{margin:63px 0 0;}
.receipt input{width:100%;margin:10px 0 30px;padding:10px;line-height:18px;font-size:14px;box-sizing:border-box;}
.receipt .center{text-align:center;}
.receipt-info{margin:20px 10px;}
.receipt-info div,.receipt-info li{margin-top:5px;font-weight:bold;}
.receipt-info i,.receipt-info em{font-weight:bold;}
.receipt-info em{margin-right:8px;}
.receipt-remark{margin:20px 10px 0;}
.receipt-remark span{display:block;margin:10px 0;}
.reject{margin:10px 0;}
.receipt-actual a{display:inline-block;width:35px;border:1px solid #bc4040;background-color:#bc4040;color:#fff;font-weight:bold;text-align:center;}
.receipt-actual input{width:45px;height:20px;line-height:18px;margin:0 -4px;padding:0;border:1px solid #bc4040;vertical-align:bottom;text-align:center;}
/*receipt end*/

/*incomes start*/
.incomes{margin:63px 0 0;}
.incomes .center{text-align:center;}
.incomes .rule{margin:15px 5px;font-weight:bold;}
.incomes .btn-red{letter-spacing:2px;}
.incomes-info{margin:15px 0 0;border:1px solid #e0e0e0;box-sizing:border-box;}
.incomes-info h2{padding:15px;text-align:center;background-color: #e0e0e0;}
/*incomessearch start*/
.incomes label{display:inline-block;width:60px;text-align:right;font-weight:bold;}
.incomes i{display:inline-block;padding:0 2px;}
.incomes input{margin:8px 0;padding:7px;line-height:18px;font-size:14px;box-sizing:border-box;}
.incomes input[type="text"]{width:50%;}
.incomes input[type="date"]{width:28.5%;}
.incomes input[type="button"]{height:30px;margin-right:4px;padding:0 6px;border:1px solid #838383;color:#838383;background-color:#fff;font-weight:bold;cursor:pointer;}
.incomes input.selected{color:#bc4040;font-weight:bold;}
.incomes .noresult-img{margin-top:125px;}
/*incomessearch end*/
/*incomesdetail start*/
.incomes-detail{background-color:#fff;}
.incomes-detail i,.incomes-detail em{font-weight:bold;}
.incomes-detail em{margin-right:8px;}
.incomes-detail .btn-red{letter-spacing:1px;}
.detail-top,.detail-bottom{padding:16px 10px;}
.detail-middle{padding:6px 10px;}
.detail-top li,.detail-middle li{line-height:22px;}
.incomes-remark{margin:10px 0;color:#bc4040;}
/*incomesdetail end*/
/*incomes end*/

/* coupon start */
.coupon{padding-top:0;margin:auto 0px;}
.unused{margin:0;}
.coupon-top{width:100%;height:48px;font-size:18px;}
.coupon-top .receive{float:right;width:17%;height:30px;min-width:100px;letter-spacing:2px;line-height:30px;margin:8px 8px 0 0;border-radius:4px;}
.coupon .header{line-height:44px;background:#fff;border-bottom:#888;border-top:#888;}
.coupon .header a{position:relative;display:inline-block;float:left;width:30%;margin-left:3%;text-align:center;font-size:16px;color:#323232;}
.coupon .header a.active{border-bottom:3px solid #bc4040;}
.coupon .header span{position:absolute;right:2px;top:5px;display:block;width:20px;font-size:14px;line-height:20px;border-radius:3px;}
.coupon-list{margin:10px 6px 10px 6px;background:url(../images/coupon-r.png) no-repeat right center;background-size:30px 100%;}
.no-coupon{position:absolute;top:0;left:0;right:0;bottom:0;width:42%;margin:50% auto;text-align:center;}
.no-coupon img{width:100%; margin-bottom:10px;}
.no-coupon p{padding-left:8px;}
.coupon-list a{display:block;width:99.5%;background:#fff;}
.coupon dl{position:relative;line-height:70px;}
.coupon dl.elapsed{width:99.5%;background: #fff url(../images/elapsed.png) no-repeat right center;background-size:77px 100%;} 
.coupon dl.expired{background: #fff url(../images/expired.png) no-repeat right center;-webkit-background-size:77px 100%;background-size:77px 100%;}
.coupon dt{width:65%;margin-left:106px;font-size:14px;line-height:24px;color:#838383;vertical-align:middle;z-index:1;background:transparent;}
.coupon dt p{width:100%;white-space:nowrap;text-overflow:ellipsis;overflow:hidden;}
.coupon dt p span:first-child{margin-right:20%;}
.coupon dt,.coupon dd{display: inline-block;height:100%;}
.coupon dd{position:absolute;top:0;left:0;width:100px;height:70px;text-align:center;line-height:80px;}
.coupon dd.coupon1{background:transparent url(../images/ico-coupon1.png) no-repeat center;-webkit-background-size:100% 100%;background-size:100% 100%;}
.coupon dd.coupon2{background:transparent url(../images/ico-coupon2.png) no-repeat center;-webkit-background-size:100% 100%;background-size:100% 100%;}
.coupon dd.coupon3{background:transparent url(../images/ico-coupon3.png) no-repeat center;-webkit-background-size:100% 100%;background-size:100% 100%;}
.coupon dd.coupon4{background:transparent url(../images/ico-coupon4.png) no-repeat center;-webkit-background-size:100% 100%;background-size:100% 100%;}
.coupon dd strong{font-size:30px;margin-left:-12px;}
.coupon dd span{position:absolute;bottom:1px;right:4px;line-height:14px;font-size:8px;}
/* .coupon detail start */
.coupon-details{line-height:32px;font-size:16px;padding:0 6%;}
.coupon-details h3{font-size:20px;line-height:72px;font-weight:normal;text-align:center;}
.coupon-details a{margin:32px auto;}
/* .coupon detail end */
/*.coupon-rules start  */
.coupon-rules{line-height:32px;font-size:16px;}
.coupon-rules h3{font-size:20px;line-height:72px;font-weight:normal;text-align:center;}
.coupon-rules a{margin:32px auto;letter-spacing:normal;}
/*.coupon-rules end  */
/* coupon end */
